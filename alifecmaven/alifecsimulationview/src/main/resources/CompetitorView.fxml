<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.*?>
<fx:root type="GridPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <columnConstraints>
        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" percentWidth="40.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="60.0" prefWidth="100.0"/>
    </columnConstraints>
    <Label fx:id="opponentName" style="-fx-font-weight: bold; -fx-font-size: 14;" text="Label"/>
    <ProgressBar fx:id="opponentsProgress" maxWidth="1.7976931348623157E308" mouseTransparent="true" progress="0.0"
                 GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"/>
    <Label fx:id="opponentsDetails" style="-fx-font-size: 12;" text="Label" GridPane.columnIndex="1"
           GridPane.rowIndex="1"/>
</fx:root>
