class LifeRule
!!!145915.java!!!	apply(inout c : Colony, inout enemy : Colony, inout mo : Cell, inout mov : Movement, inout mitosis : boolean) : boolean
      if(c == null || mo == null || mo.pos == null )
         throw new IllegalArgumentException("Illegal Argument");
      
      mo.ene -= Defs.LESS_LIVE;
      
      if(mo.ene <= 0.0f){
         env.killMO(mo.pos.x, mo.pos.y);
         return true;
      }
	  
      return false;
